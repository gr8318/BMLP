import numpy as np

A = np.array([[1, 2], [3, 4]])
B = np.array([[4, 5], [6, 7]])
print("A + B:\n", A + B)
print("A - B:\n", A - B)

C = np.array([[1, 2], [3, 4], [5, 6]])
D = np.array([0.5, 0.5])
print("Dot Product:\n", C.dot(D))

print("Scalar Multiplication:\n", C * 0.5)

E = np.arange(4).reshape(2, 2)
print("Transpose:\n", E.T)

